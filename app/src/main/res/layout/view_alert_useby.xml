<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <ImageView
        android:id="@+id/image_useby"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/abc_btn_radio_material" />

    <TextView
        android:id="@+id/text_cosmetic"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="Cosmetic" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="@string/view_alert_useby_sentence" />

    <TextView
        android:id="@+id/text_useby"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="Date" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="@string/view_alert_useby_sentence_two" />

</LinearLayout>