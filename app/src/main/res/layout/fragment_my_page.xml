<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mypage.MyPageFragment">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="alarm"
            android:id="@+id/btn_alarm"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="modification"
            android:id="@+id/btn_modification"
            android:layout_marginLeft="30dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="70dp" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="setting"
            android:id="@+id/btn_setting"
            android:layout_alignTop="@+id/btn_modification"
            android:layout_toRightOf="@+id/btn_modification"
            android:layout_toEndOf="@+id/btn_modification" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="my cosmetic"
            android:id="@+id/btn_my_cosmetic"
            android:layout_below="@+id/btn_modification"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="37dp" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="tip"
            android:id="@+id/btn_tip"
            android:layout_below="@+id/btn_my_cosmetic"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="99dp" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="like"
            android:id="@+id/btn_like"
            android:layout_alignBottom="@+id/btn_tip"
            android:layout_toLeftOf="@+id/btn_alarm"
            android:layout_toStartOf="@+id/btn_alarm" />
    </RelativeLayout>

</FrameLayout>
